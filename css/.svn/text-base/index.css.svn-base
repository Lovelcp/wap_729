* { margin:0; padding:0; font-size:12px ; font-family:"\5B8B\4F53", Arial; font-weight:normal; list-style:none;font-style: normal; }
a img{ vertical-align:top; border:0; }
a {text-decoration:none;}
body{background: #333;}
.wrap{width:320px;margin:0 auto;overflow: hidden;}
.header_img{height:95px;width:320px;}
.gonggao{TEXT-ALIGN: center;PADDING-BOTTOM: 5px;COLOR: #ffffff;FONT-SIZE: 12px;font:14px/20px "微软雅黑","宋体";margin:10px 0;}
.gonggao strong{font:14px/20px "微软雅黑","宋体";}
.gonggao span{font:14px/20px "微软雅黑","宋体";color:#eb6100;margin:0 5px;}
.list{height:261px;margin-top:5px;}
.list a{display: block;width:158px;height:85px;float:left;_display:inline;margin:0 1px 2px 1px;position: relative;}
.list a img{width:158px;height:85px;border:0;}
.list a span{position: absolute;width:158px;height:20px;left:0;bottom: 0;font:16px/20px "微软雅黑";color:#fff;background: #333333;opacity: .7;text-align: center;}
.list a samp{position: absolute;width:158px;height:20px;left:0;bottom: 0;font:16px/20px "微软雅黑";color:#fff;text-align: center;}
.xz{height:55px;position:relative;background:#ccc;}
.xz a{position: absolute;display: block;width:155px;height:35px;left:50%;top:10px;background: url("../images/down.jpg") no-repeat;margin-left:-77px;}
.app{background: url("../images/bottom.gif") center center no-repeat;height: 90px;display: block;}
.f_bottom_gg{position:fixed;height:58px;width:100%;z-index:99999;left:0;bottom:0;overflow:hidden;}
.f_bottom_zw{height:58px;}